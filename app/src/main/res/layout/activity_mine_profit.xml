                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_5"
                android:gravity="center"
                android:text="0.00"
                android:textColor="#ffffffff"
                android:textSize="@dimen/sp_18"/>

        </LinearLayout>

